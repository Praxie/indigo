<table class="table">
	<%- @posts.each do |post| %>
	<div class = "one_post">
		<h1><%= link_to post.title, post_path(post) %></h1>
		<%= link_to 'Edit', edit_post_path(post) %>
		<%= link_to 'Delete', post_path(post),
		                method: :delete, data: { confirm: 'Are you sure?' } %>
		<p class = "image">
  		<%= image_tag("#{ post.content }") %>
		</p>	
	</div>
	<% end %>
	<%= will_paginate @posts %>
</table>
<%= link_to "Home", root_path %>